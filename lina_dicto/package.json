{
  "name": "lina_dicto",
  "version": "0.2.6",
  "description": "Dictionary of Japanese to Esperanto",
  "main": "index.js",
  "private": true,
  "scripts": {
    "running": "electron .",
    "build": "npm run build:osx && npm run build:win && npm run build:linux",
    "debian_installer_debian": "electron-installer-debian --config ./installer_debian_amd64_config.json",
    "test": "mocha --require intelli-espower-loader",
    "clean": "rm -rf ./release",
    "clean:osx": "rm -rf ./release/osx",
    "clean:mas": "rm -rf ./release/mas",
    "clean:win": "rm -rf ./release/win",
    "clean:linux": "rm -rf ./release/linux",
    "pack": "npm run pack:osx && npm run pack:win && npm run pack:linux",
    "pack:win": "npm run clean:win && electron-packager . --out=./release/win --platform=win32 --arch=x64 --icon='./image/icon.ico' --ignore='release' --ignore='object' --ignore='work' --ignore='test'",
    "pack:osx": "npm run clean:osx && electron-packager . --out=./release/osx --platform=darwin --arch=x64 --icon='./image/icon.icns' --ignore='release/' --ignore='object/' --ignore='work/' --ignore='test/'",
    "pack:mas": "npm run clean:mas && electron-packager . --out=./release/mas --platform=mas --arch=x64 --icon='./image/icon.icns' --ignore='release/' --ignore='object/' --ignore='work/' --ignore='test/' --ignore='\\.o(bj)?$' --app-bundle-id='com.michinari-nukazawa.lina-dicto-for-desktop'",
    "pack:linux": "npm run clean:linux && electron-packager . --out=./release/linux --platform=linux --arch=x64 --icon='./image/icon.png' --ignore='release/' --ignore='object/' --ignore='work/' --ignore='test/'",
    "testtotest": "mocha test/mocha.js"
  },
  "author": "michinari.nukazawa@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichinariNukazawa/lina_dicto.git"
  },
  "bugs": {
    "url": "https://github.com/MichinariNukazawa/lina_dicto/issues"
  },
  "keywords": [
    "esperanto",
    "dictionary"
  ],
  "license": "BSD-2-Clause",
  "directories": {
    "test": "test/"
  },
  "dependencies": {
    "about-window": "^1.13.2",
    "kuromoji": "^0.1.2",
    "write-int": "0.0.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "electron": "^7.1.4",
    "electron-installer-debian": "^2.0.1",
    "electron-packager": "^14.1.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^6.2.2",
    "power-assert": "^1.6.1",
    "spectron": "^9.0.0"
  }
}
